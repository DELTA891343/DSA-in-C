#include <stdio.h>
#include <conio.h>
void main()
{
  int arr[10]={11,33,22,66,44,99,77,55,88,100};
  int mid,low=0,high=9,num;
  clrscr();
  printf("enter number to be search);
  scanf("%d",&num);
  mid=(low+high)/2;

  while(low<=high)
  {
    if(arr[mid]==high)
      low=mid+1;
        else if(arr[mid]==num)
        {
            pritnf("number is not found at %d",mid);
        }
        else
        {
            printf("number found at %d",mid);
        }
        else
        {
            high=mid-1;
        }
        mid=(low>high)
        {
            if(low>high)
            {
              printf("data not found");
            }
        }
getch();

}

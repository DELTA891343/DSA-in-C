#include <stdio.h>
#include <conio.h>
void main()
{
  int arr[5]={65,35,97,23,13,};
  int i,j,temp;
  clrscr();
  printf("before sorting");
  for(i=o;i<=4;i++)
  {  
    for(j=0;j<=4;j++)
      {
          if(arr[i]>arr[j])
            {
              temp=arr[i];
              arr[i]=arr[j];
              arr[j]=temp;
            }
      }
  }
  printf("\n after sorting \n");
  for(i=0;i<5;i++)
  {
    printf("%d",arr[i]);
  }
getch();
}

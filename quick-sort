}
int split(int a[], int lower,int upper
{
  int pivot,start,end,t;
  start=upper;
  end=upper;
  pivot=a[lower];
  while(start<end)
  {
    while(a[strat]<=pivot)
    {
      strat ++;
    }
  while(a [end] > pivot)
  {
    t=a[start];
    a=[loweer]=a[end];
    a[end]=t;
    return end;
}
